<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Ashlands_KwamaNest]==========-->
	<MapGeneratorDef>
		<defName>Mashed_Ashlands_KwamaNest</defName>
		<label>kwama nest</label>
		<isUnderground>true</isUnderground>
		<forceCaves>true</forceCaves>
		<ignoreAreaRevealedLetter>true</ignoreAreaRevealedLetter>
		<disableCallAid>true</disableCallAid>
		<!--
		<customMapComponents>
			<li>UndercaveMapComponent</li>
		</customMapComponents>
		-->
		<pocketMapProperties>
			<biome>Mashed_Ashlands_KwamaNest</biome>
			<temperature>15</temperature>
		</pocketMapProperties>
		<genSteps>
			<li>ElevationFertility</li>
			<li>Undercave_RocksFromGrid</li>
			<li>Undercave_Caves</li>
			<li>Terrain</li>
			<li>Mashed_Ashlands_KwamaNestTerrain</li>
			<li>RockChunks</li>
			<li>Mashed_Ashlands_FindKwamaNestExit</li>
			<li>Mashed_Ashlands_KwamaQueen</li>
			<li>Mashed_Ashlands_KwamaNestExplorer</li>
			<li>Mashed_Ashlands_KwamaEggSacs</li>
			<li>Mashed_Ashlands_GoldKwamaEggSacs</li>
			<!-- warriors -->
			<!-- <li>UndercaveInterest</li> -->
			<li>Plants</li>
			<li>Animals</li>
			<li>Fog</li>
		</genSteps>
	</MapGeneratorDef>
	<!-- ==========[Mashed_Ashlands_KwamaNestTerrain]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_KwamaNestTerrain</defName>
		<order>211</order>
		<genStep Class="Mashed_Ashlands_Kwama.GenStep_NestTerrain"/>
	  </GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaNestExit]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_FindKwamaNestExit</defName>
		<order>300</order>
		<genStep Class="Mashed_Ashlands_Kwama.GenStep_FindKwamaNestExit"/>
	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaQueen]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_KwamaQueen</defName>
		<order>600</order>
		<genStep Class="Mashed_Ashlands_Kwama.GenStep_KwamaQueen"/>
	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaNestExplorer]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_KwamaNestExplorer</defName>
		<order>610</order>
		<genStep Class="GenStep_ScatterThings">
			<thingDef>Mashed_Ashlands_KwamaExplorerSkeleton</thingDef>
			<spotMustBeStandable>true</spotMustBeStandable>
			<allowFoggedPositions>false</allowFoggedPositions>
			<count>1</count>
			<validators>
				<li Class="ScattererValidator_Buildable">
					<radius>3</radius>
					<affordance>Heavy</affordance>
				</li>
			</validators>
		</genStep>
	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaEggSacs]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_KwamaEggSacs</defName>
		<order>611</order>
		<genStep Class="GenStep_ScatterThings">
			<thingDef>Mashed_Ashlands_KwamaEggSac</thingDef>
			<spotMustBeStandable>true</spotMustBeStandable>
			<countPer10kCellsRange>60~90</countPer10kCellsRange>
			<clusterSize>3</clusterSize>
			<validators>
				<li Class="ScattererValidator_Buildable">
					<radius>1</radius>
					<affordance>Heavy</affordance>
				</li>
			</validators>
		</genStep>
	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_GoldKwamaEggSacs]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_GoldKwamaEggSacs</defName>
		<order>612</order>
		<genStep Class="GenStep_ScatterThings">
			<thingDef>Mashed_Ashlands_GoldKwamaEggSac</thingDef>
			<spotMustBeStandable>true</spotMustBeStandable>
			<count>3</count>
			<validators>
				<li Class="ScattererValidator_Buildable">
					<radius>1</radius>
					<affordance>Heavy</affordance>
				</li>
			</validators>
		</genStep>
	</GenStepDef>
</Defs>