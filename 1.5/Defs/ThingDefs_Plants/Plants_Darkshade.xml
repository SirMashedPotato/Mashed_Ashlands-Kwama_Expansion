	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeDraggleTails]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_DraggleTailsBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeDraggleTails</defName>
		<label>darkshade draggle-tails</label>
		<description>A rare strain of the common draggle-tails, only found growing in darklight caverns beneath the ashlands. They produce a strange pale glow, that while somewhat unsettling, can help navigate dark caverns.</description>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeDraggleTails</texPath>
		</graphicData>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
		</plant>
		<comps Inherit="False">
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
			<li Class="Mashed_Ashlands.CompProperties_EffecterAtNight">
				<effecterDef>Mashed_Ashlands_DarklightTorchbugEffecter</effecterDef>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeEntoloma]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeEntoloma</defName>
		<label>darkshade entoloma</label>
		<description>A small mushroom, only found growing in darklight caverns beneath the ashlands. While the strange pale glow it produces might be somewhat unsettling to some, most find its striking colouration quite beautiful.</description>
		<statBases>
			<Beauty>10</Beauty>
			<BeautyOutdoors>10</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_DarkshadeEntoloma</texPath>
		</graphicData>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<growDays>3</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
			<purpose>Beauty</purpose>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeButtercake]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeButtercake</defName>
		<label>darkshade buttercake mushroom</label>
		<description>A rare strain of the buttercake mushroom, only found growing in darklight caverns beneath the ashlands. Despite its eerie glow, the flesh harvested from it is just as tasty, and still safe to eat While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.25</Nutrition>
			<Mashed_Ashlands_AshResistance>0.6</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeButtercakePlant</texPath>
		</graphicData>
		<pathCost>14</pathCost>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<fertilitySensitivity>0.8</fertilitySensitivity>
			<fertilityMin>1</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestedThingDef>Mashed_Ashlands_RawDarkshadeButtercake</harvestedThingDef>
			<harvestYield>8</harvestYield>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.05</topWindExposure>
			<growDays>7</growDays>
			<visualSizeRange>0.8~1.1</visualSizeRange>
			<purpose>Food</purpose>
			<humanFoodPlant>true</humanFoodPlant>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>200</wildClusterWeight>
			<wildOrder>2</wildOrder>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeMushroom]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeMushroom</defName>
		<label>darkshade mushroom</label>
		<description>A small edible mushroom, only found growing in darklight caverns beneath the ashlands. Its flesh is edible, though most don't consider it very pleasant to eat. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.25</Nutrition>
			<Mashed_Ashlands_AshResistance>0.6</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeMushroomPlant</texPath>
		</graphicData>
		<pathCost>14</pathCost>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<fertilitySensitivity>0.8</fertilitySensitivity>
			<fertilityMin>1</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestedThingDef>Mashed_Ashlands_RawDarkshadeMushroom</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.05</topWindExposure>
			<growDays>7</growDays>
			<maxMeshCount>4</maxMeshCount>
			<visualSizeRange>0.4~0.8</visualSizeRange>
			<purpose>Food</purpose>
			<humanFoodPlant>true</humanFoodPlant>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>200</wildClusterWeight>
			<wildOrder>2</wildOrder>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeParasolBush]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_ParasolBushBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeParasolBush</defName>
		<label>darkshade parasol bush</label>
		<description>A rare strain of the parasol bush, only found growing in darklight caverns beneath the ashlands. The stem can be harvested for parasol wood, though it takes slightly longer to harden than the regular parasol bush. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<Mashed_Ashlands_AshResistance>0.8</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeParasolBush</texPath>
		</graphicData>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<growDays>9</growDays>
			<sowResearchPrerequisites Inherit="False">
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeReed]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PseudoTreeBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeReed</defName>
		<label>darkshade reed plant</label>
		<description>A rare type of reed, only found growing in darklight caverns beneath the ashlands. Sometimes used as a supplement for wood. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<statBases>
			<Beauty>3</Beauty>
			<BeautyOutdoors>3</BeautyOutdoors>
			<MaxHitPoints>45</MaxHitPoints>
			<Nutrition>0.25</Nutrition>
			<Mashed_Ashlands_AshResistance>0.6</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeReedPlant</texPath>
		</graphicData>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<fertilitySensitivity>1</fertilitySensitivity>
			<fertilityMin>0.8</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestedThingDef>WoodLog</harvestedThingDef>
			<harvestYield>3</harvestYield>
			<sowMinSkill>10</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<growDays>8</growDays>
			<visualSizeRange>1~2.2</visualSizeRange>
			<maxMeshCount>4</maxMeshCount>
			<blockAdjacentSow>false</blockAdjacentSow>
			<wildClusterRadius>6</wildClusterRadius>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeScathecraw]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_ScathecrawBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeScathecraw</defName>
		<label>darkshade scathecraw plant</label>
		<description>A rare strain of scathecraw, only found growing in darklight caverns beneath the ashlands. Does not yield as much edible flesh as the regular scathecraw, though it is able to grow in near darkness. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeScathecrawPlant</texPath>
		</graphicData>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<harvestYield>6</harvestYield>
			<harvestedThingDef>Mashed_Ashlands_RawDarkshadeScathecraw</harvestedThingDef>
			<sowResearchPrerequisites Inherit="False">
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<visualSizeRange>0.8~1.5</visualSizeRange>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DarkshadeTramaShrub]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_TramaShrubBase">
		<defName>Mashed_Ashlands_Plant_DarkshadeTramaShrub</defName>
		<label>darkshade trama shrub</label>
		<description>A rare strain of trama shrub, only found growing in darklight caverns beneath the ashlands. The roots are comparatively smaller than that of the regular trama shrub, though it is able to grow in near darkness. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<statBases>
			<Beauty>1</Beauty>
			<BeautyOutdoors>1</BeautyOutdoors>
			<Mashed_Ashlands_AshResistance>0.6</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Ashlands_DarkshadeTramaShrub</texPath>
		</graphicData>
		<plant>
			<harvestYield>4</harvestYield>
			<harvestedThingDef>Mashed_Ashlands_RawDarkshadeTramaRoot</harvestedThingDef>
			<sowResearchPrerequisites Inherit="False">
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_GiantDarkshadeCoprinus]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_TreeDecorativeBase">
		<defName>Mashed_Ashlands_GiantDarkshadeCoprinus</defName>
		<label>darkshade coprinus tree</label>
		<description>A large fungal tree, only found growing in darklight caverns beneath the ashlands. While it offers nothing of practical use, most find it incredibly beautiful, though some find the strange pale glow it emits slightly unsettling. While it can only be found growing naturally underground, with enough care it can be grown almost anywhere.</description>
		<graphicData>
			<texPath>Things/Plant/Trees/Mashed_Ashlands_GiantDarkshadeCoprinus</texPath>
		</graphicData>
		<statBases>
			<Beauty>25</Beauty>
			<BeautyOutdoors>25</BeautyOutdoors>
			<Mashed_Ashlands_AshResistance>0.6</Mashed_Ashlands_AshResistance>
		</statBases>
		<plant>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0.5</growOptimalGlow>
			<growDays>20</growDays>
			<visualSizeRange>1.3~2.4</visualSizeRange>
			<harvestWork>600</harvestWork>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Darkshade</li>
			</sowResearchPrerequisites>
			<purpose>Beauty</purpose>
			<leaflessGraphicPath>Things/Plant/Trees/Mashed_Ashlands_GiantDarkshadeCoprinus_Leafless</leaflessGraphicPath>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>3</glowRadius>
				<glowColor>(78,226,229,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
