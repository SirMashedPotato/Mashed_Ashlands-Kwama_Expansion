				<eggFertilizedDef>Mashed_Ashlands_Egg_NixOxDarkshadeFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>3.3</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1~2</eggCountRange>
			</li>
			<li Class="CompProperties_Shearable">
				<woolDef>Mashed_Ashlands_NixOxChitin</woolDef>
				<shearIntervalDays>10</shearIntervalDays>
				<woolAmount>100</woolAmount>
