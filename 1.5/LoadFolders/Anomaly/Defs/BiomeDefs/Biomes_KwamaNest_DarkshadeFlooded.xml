<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<BiomeDef ParentName="Mashed_Ashlands_KwamaNestBase">
		<defName>Mashed_Ashlands_DarkshadeFloodedKwamaNest</defName>
		<label>flooded darkshade kwama nest</label>
		<plantDensity>1.2</plantDensity>
		<modExtensions>
			<li Class="Mashed_Ashlands.BiomeProperties">
				<forceCustomCavePlants>true</forceCustomCavePlants>
				<cavePlants>
					<Mashed_Ashlands_Plant_Chokevine>0.2</Mashed_Ashlands_Plant_Chokevine>
					<Mashed_Ashlands_Plant_DarkshadeDraggleTails>0.05</Mashed_Ashlands_Plant_DarkshadeDraggleTails>
					<Mashed_Ashlands_Plant_DarkshadeEntoloma>0.2</Mashed_Ashlands_Plant_DarkshadeEntoloma>
					<Mashed_Ashlands_Plant_DarkshadeMushroom>0.1</Mashed_Ashlands_Plant_DarkshadeMushroom>
					<Mashed_Ashlands_Plant_DarkshadeParasolBush>0.05</Mashed_Ashlands_Plant_DarkshadeParasolBush>
					<Mashed_Ashlands_DarkshadeDuskCap>0.1</Mashed_Ashlands_DarkshadeDuskCap>
					<Mashed_Ashlands_GiantDarkshadeCoprinus>0.2</Mashed_Ashlands_GiantDarkshadeCoprinus>
					<Mashed_Ashlands_Plant_TreeDarkshadeNetchShield>0.025</Mashed_Ashlands_Plant_TreeDarkshadeNetchShield>
					<Mashed_Ashlands_Plant_MuckSponge>0.05</Mashed_Ashlands_Plant_MuckSponge>
					<Mashed_Ashlands_Plant_DarkshadeGlowstalk MayRequire="Ludeon.RimWorld.Ideology">0.05</Mashed_Ashlands_Plant_DarkshadeGlowstalk>
					<!-- Polluted plants -->
					<Mashed_Ashlands_Plant_GlowingBryolux MayRequire="Ludeon.RimWorld.Biotech">0.1</Mashed_Ashlands_Plant_GlowingBryolux>
					<Mashed_Ashlands_Plant_GlowingDraggleTails MayRequire="Ludeon.RimWorld.Biotech">0.05</Mashed_Ashlands_Plant_GlowingDraggleTails>
					<Mashed_Ashlands_Plant_GlowingRussula MayRequire="Ludeon.RimWorld.Biotech">0.2</Mashed_Ashlands_Plant_GlowingRussula>
					<Mashed_Ashlands_Plant_GlowingCoprinus MayRequire="Ludeon.RimWorld.Biotech">0.2</Mashed_Ashlands_Plant_GlowingCoprinus>
					<Mashed_Ashlands_Plant_GlowingParasolBush MayRequire="Ludeon.RimWorld.Biotech">0.1</Mashed_Ashlands_Plant_GlowingParasolBush>
					<Mashed_Ashlands_GiantGlowingCoprinus MayRequire="Ludeon.RimWorld.Biotech">0.2</Mashed_Ashlands_GiantGlowingCoprinus>
				</cavePlants>
				<dryToReplacers>
					<li>
						<originalTerrain>Gravel</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_RichAshGravel</replacedTerrain>
					</li>
					<li>
						<originalTerrain>Soil</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_DarkshadeMossyAshSoil</replacedTerrain>
					</li>
				</dryToReplacers>
				<increasePollutedFertility>true</increasePollutedFertility>
				<otherTerrainDef>MarshyTerrain</otherTerrainDef>
				<otherTerrainThreshold>0.05</otherTerrainThreshold>
				<waterTerrainDef>Mashed_Ashlands_ShallowMarsh</waterTerrainDef>
				<waterThreshold>0.4</waterThreshold>
			</li>
			<li Class="Mashed_Ashlands_Kwama.UnderBiomeProperties">
				<wildAnimals>
					<li>
						<kindDef>Rat</kindDef>
						<weight>0.2</weight>
						<maxOnMap>3</maxOnMap>
					</li>
					<li>
						<kindDef>Mashed_Ashlands_MudcrabDarkshade</kindDef>
						<weight>0.3</weight>
						<maxOnMap>3</maxOnMap>
					</li>
					<li>
						<kindDef>Mashed_Ashlands_KwamaScrib</kindDef>
						<weight>0.6</weight>
						<maxOnMap>6</maxOnMap>
					</li>
					<li>
						<kindDef>Mashed_Ashlands_KwamaForager</kindDef>
						<weight>0.8</weight>
						<maxOnMap>3</maxOnMap>
					</li>
					<li>
						<kindDef>Mashed_Ashlands_KwamaWorker</kindDef>
						<weight>1</weight>
						<maxOnMap>6</maxOnMap>
					</li>
					<li>
						<kindDef>Mashed_Ashlands_KwamaWarrior</kindDef>
						<weight>1</weight>
						<maxOnMap>3</maxOnMap>
					</li>
				</wildAnimals>
			</li>
		</modExtensions>
	</BiomeDef>
</Defs>