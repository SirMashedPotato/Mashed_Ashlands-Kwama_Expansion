				<li Class="ScattererValidator_Buildable">
					<radius>1</radius>
					<affordance>Heavy</affordance>
				</li>
			</validators>
		</genStep>
	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaBurrow]==========-->
