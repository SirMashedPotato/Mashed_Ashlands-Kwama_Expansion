	</GenStepDef>
	<!-- ==========[Mashed_Ashlands_KwamaNestTerrain]==========-->
	<GenStepDef>
		<defName>Mashed_Ashlands_KwamaNestTerrain</defName>
		<order>211</order>
		<genStep Class="Mashed_Ashlands_Kwama.GenStep_NestTerrain"/>
	</GenStepDef>
